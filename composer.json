{
  "name": "nejcc/php-custom-types",
  "description": "A Laravel package providing custom data types like i8, u8, i32, u32, f32, etc.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nejc",
      "email": "your-email@example.com"
    }
  ],
  "require": {
    "php": ">=8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Nejcc\\CustomTypes\\Int\\": "src/Int/",
      "Nejcc\\CustomTypes\\Float\\": "src/Float/",
      "Nejcc\\CustomTypes\\String\\": "src/String/",
      "Nejcc\\CustomTypes\\Tuple\\": "src/Tuple/",
      "Nejcc\\CustomTypes\\Array\\": "src/Array/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nejcc\\CustomTypes\\CustomTypesServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
